{
  "name": "yesplash",
  "version": "1.0.0",
  "description": "yes !! i can use unsplash wallpaper.",
  "main": "UI.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dist": "electron-builder --win --x64"
  },
  "build": {
    "appId": "com.sam.yesplash",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "zip"
      ],
      "icon": "icon.ico"
    },
    "extraResources": {
      "from": "./config.yml",
      "to": "../config.yml"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sammeishi/yesplash.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sammeishi/yesplash/issues"
  },
  "homepage": "https://github.com/sammeishi/yesplash#readme",
  "dependencies": {
    "file-saver": "^2.0.2",
    "find-remove": "^2.0.0",
    "fs-extra": "^8.1.0",
    "http-proxy-agent": "^4.0.0",
    "https-proxy-agent": "^4.0.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "socks5-node-fetch": "^0.0.4",
    "unsplash-js": "^6.0.0",
    "wallpaper": "^4.4.1"
  },
  "devDependencies": {
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2"
  }
}
